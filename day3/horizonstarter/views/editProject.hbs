<h2>Edit project</h2>

<!-- Part 2: Create project -->

<form method="POST" action="/project/{{projectid}}/edit">
	<div>
	<div> <labelvalue="{{projectid}}">project id</label></div>
	<input type="text" name="title" placeholder="Title" value="{{title}}">
	</div>
	<div>
	<input type="text" name="goal" placeholder="Goal" value="{{goal}}">
	</div>

	<div class="form-group">
        <label for="category">Category</label>
        <select id="category" name="category" class="form-control" value="{{category}}">
      <option>Famous Muppet Frogs</option>
      <option>Current Black Presidents</option>
      <option>The Pen Is Mightier</option>
      <option>Famous Mothers</option>
      <option>Drummers Named Ringo</option>
      <option>1-Letter Words</option>
      <option>Months That Start With "Feb"</option>
      <option>How Many Fingers Am I Holding Up</option>
      <option>Potent Potables</option>
    </div>


	<div>
	<textarea name="description" value="{{description}}"></textarea>
	</div>
	<div>
	<input type="date" name="start" placeholder="Start Date" value="{{start}}">
	</div>
	<div>
	<input type="date" name="end" placeholder="End Date" value="{{end}}">
	</div>
	<input type="Submit" name="Submit">



	
</form>

