<h2>Edit project</h2>

<form method="POST" action="/project/{{projID}}/edit">
  <div class="form-group container">
    Title: <input type="text" class="form-control" name="title" value="{{project.title}}">
    Goal amount: <input type="text" class="form-control" name="goal" value="{{project.goal}}" placeholder="should be an amount in USD">
    Description: <textarea class="form-control" rows="3" name="description" value="project.description">{{project.description}}
    </textarea>
    Start Date: <input type="date" class="form-control" name="start" value="{{project.start}}">
    End Date: <input type="date" class="form-control" name="end" value="{{project.end}}">
    <div class="form-group">
        <label for="category">Category</label>
        <select class="form-control" id="category" name="categoryz">
          <option value="Famous Muppet Frogs">Famous Muppet Frogs</option>
          <option value="Current Black Presidents">Current Black Presidents</option>
          <option value="The Pen Is Mightier">The Pen Is Mightier</option>
          <option value="Famous Mothers">Famous Mothers</option>
          <option value="Drummers Named Ringo">Drummers Named Ringo</option>
          <option value="1-Letter Words">1-Letter Words</option>
          <option value="Months That Start With 'Feb'">Months That Start With "Feb"</option>
          <option value="How Many Fingers Am I Holding Up">How Many Fingers Am I Holding Up</option>
          <option value="Potent Potables">Potent Potables</option>
        </select>
      </div>
    <input type="submit" value="Update project" class="btn btn-default">
  </div>
</form>
