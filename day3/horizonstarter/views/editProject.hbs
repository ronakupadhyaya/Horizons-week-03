<h2>Create project</h2>

<form method="POST" action="/projects/{{projectid}}/edit">
  <input type="text" name="title" placeholder="Title" value={{title}}>
  <input type="text" name="goal" placeholder="Goal" value={{goal}}>
  <textarea type="text" name="description" value="description">{{description}}</textarea>
  <input type="Date" name="start" placeholder="Start" value='{{start}}'>
  <input type="Date" name="end" placeholder="End" value='{{end}}'>
  <select name="category" placeholder="Project category" value="Category">
    {{{category}}}

  </select>
  <button type="submit">Submit</button>
</form>
