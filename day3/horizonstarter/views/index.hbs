<h1>View projects</h1>
<a href="http://localhost:3000/?sort=start{{#if dir}}&sortDirection={{dir}}{{/if}}"> start</a>
<a href="http://localhost:3000/?sort=end{{#if dir}}&sortDirection={{dir}}{{/if}}"> end</a>
<a href="http://localhost:3000/?sort=goal{{#if dir}}&sortDirection={{dir}}{{/if}}"> goal</a>

<a href="http://localhost:3000/{{#if sort}}?sort={{sort}}&sortDirection=asc{{/if}}"> ascending</a>
<a href="http://localhost:3000/{{#if sort}}?sort={{sort}}&sortDirection=dec{{/if}}"> descending</a>

<a href="http://localhost:3000/?sort=totalcontribution{{#if dir}}&sortDirection={{dir}}{{/if}}"> total-contribution</a>


<a href="/funded/funded=true"> fully funded</a>
<a href="/funded/funded=false"> not fully founded</a>
{{#each items}}
<div>
<span>Title: </span>
<span>{{this.title}}</span>
<div>
<a href="http://localhost:3000/project/{{this.id}}">go to project</a>
</div>
</div>
{{/each}}
