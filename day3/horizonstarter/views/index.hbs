<h1>View projects</h1>

<!-- Part 1: View all projects -->
<!-- YOUR CODE HERE -->
<div class="project-container">

  {{#each project_arr}}

    <div class="project-index">
      <h4>{{this.title}}</h4>
      <p> Goal: {{this.goal}} </p>
      <p> Description: {{this.description}} </p>
      <p> Start Date: {{this.start}} </p>
      <p> End Date: {{this.end}} </p>
    </div>

  {{/each}}

</div>

<!-- Part 2: Create new project -->
<!-- YOUR CODE HERE -->


<a class="new-project col-xs-offset-5 btn btn-default" href="/new"> Create a new project </a>
