<h1>View projects</h1>

<div class="row">
{{#each projects}}
  <div class="col-xs-3 col-sm-3 col-md-4 col-lg-4">
    <div class="thumbnail">
      <!-- <img src="..." alt="..."> -->
      <h3 class = "proj-title">{{this.title}}</h3>
      <div class="caption">
        <!-- <p>{{this.description}}</p> -->
        <p><a href="/project/{{_id}}" class="btn btn-primary" role="button">See details</a></p>
      </div>
    </div>
  </div>
{{/each}}
</div>

