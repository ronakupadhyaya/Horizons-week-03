<h2>Create project</h2>

<!-- Part 2: Create project -->
<!-- Part 5: Project categories -->

<form class="create_project" action="/new" method="post">
  <h3>Title</h3>
  <input type="text" name="title" value="{{title}}" placeholder="Title"><br>
  <h3>Goal</h3>
  <input type="text" name="goal" value="{{goal}}" placeholder="Goal"><br> <br>
  <textarea name="description" rows="8" cols="80" placeholder="Enter description here...">{{description}}</textarea><br>
  <h3>Start</h3>
  <input type="date" name="start" value="{{start}}"> <br>
  <h3>End</h3>
  <input type="date" name="end" value="{{end}}"> <br>
  <h3>Category</h3>
  <select name="category">
      <option value="Famous Muppet Frogs">Famous Muppet Frogs</option>
      <option value="Current Black Presients">Current Black Presidents</option>
      <option value="The Pen Is Mightier">The Pen Is MightierFamous Mothers</option>
      <option value="Famous Mothers">Famous Mothers</option>
      <option value="Drummers Named Ringo">Drummers Named Ringo</option>
      <option value="1-Letter Words">1-Letter Words</option>
      <option value='Months That Start With "Feb"'>Months That Start With "Feb"</option>
      <option value="How Many Fingers Am I Holding Up">How Many Fingers Am I Holding Up</option>
      <option value="Potent Potables">Potent Potables</option>
</select>
<br><br>
<input type="submit" name="submit" value="Post">

</form>
