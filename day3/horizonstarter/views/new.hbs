<h2>Create project</h2>

{{#if errors}}
  <form action = "/new" method = "post">
    <input type="text" value="{{project.title}}" name="title" placeholder="Title"/>
    <input type="number" value="{{project.goal}}" name="goal" placeholder="Goal"/>
    <textarea rows="4" cols = "50" value="{{project.textarea}}" name="textarea" placeholder="Description"></textarea>
    <input type="date" value="{{project.start}}" name="start"/>
    <input type="date" value="{{project.end}}" name="end"/>
    <input type="submit" value="Login"/>
  </form>
{{else}}
  <form action = "/new" method = "post">
    <input type="text" value="" name="title" placeholder="Title"/>
    <br>
    <input type="number" value="" name="goal" placeholder="Goal"/>
    <br>
    <textarea rows="4" cols = "50" value="" name="textarea" placeholder="Description"></textarea>
    <br>
    <input type="date" value="" name="start"/>
    <br>
    <input type="date" value="" name="end"/>
    <br>
    <select name = "projectlist">
      <option value="">Project Types</option>
      <option value="Famous Muppet Frog">Famous Muppet Frog</option>
      <option value="Current Black Presidents">Current Black Presidents</option>
      <option value="The Pen Is Mightier">The Pen Is Mightier</option>
      <option value="Drummers Named Ringo">Drummers Named Ringo</option>
      <option value="1-Letter Words">1-Letter Words</option>
      <option value='Months That Start With "Feb"'>Months That Start With "Feb"</option>
      <option value="How Many Fingers Am I Holding Up">How Many Fingers Am I Holding Up</option>
      <option value="Potent Potables">Potent Potables</option>
    </select>
    <br>
    <input type="submit" value="Submit"/>
  </form>
{{/if}}
