<h2>create project</h2>

{{#if errors}}
  {{#each errorArr}}
    {{> "warning" msg=msg}}
  {{/each}}
{{/if}}
<!-- Part 2: Create project -->
<div class="create-form">
  <form action="/new" method="post">
    <input class="title" type="text" name="title" placeholder="title" value="{{title}}"><br>
    <input class="goal" type="text" name="goal" placeholder="goal" value="{{goal}}"><br>
    <textarea name="description" placeholder="description..." value="{{description}}"></textarea><br>
    <input class="start" type="date" name="start" value="{{start}}"><br>
    <input class="end" type="date" name="end" value="{{end}}"><br></br>
    <select class="selectpicker" name="categories">
      {{#each enumArr}}
        <option value="{{this}}">{{this}}</option>
      {{/each}}
