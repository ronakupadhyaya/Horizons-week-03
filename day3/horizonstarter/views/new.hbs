<form action='/new' method='POST'>
  <div>
  Title: <input type="text" name="title" value="{{p.title}}" placeholder="title"/>
  Goal: <input type="text" name="goal" value="{{p.goal}}" placeholder="goal"/>
  Description: <textarea rows="5" cols="100" value="{{p.description}}"></textarea>
  Start: <input type="date" name="start" value="{{p.start}}"/>
