<h2>Create project</h2>

<!-- Part 2: Create project -->
<form method="post" action="/new">
    <input type="text" name="title" value="{{title}}" placeholder="Title">
    <input type="number" name="goal" value="{{goal}}" placeholder="Goal">
    <textarea name="description" rows="1" cols="40"></textarea>
    <input type="Date" name="start" value="{{start}}" placeholder="Start date">
    <input type="Date" name="end" value="{{end}}" placeholder="End date">
    <input type="submit" name="" value="Submit">
</form>
{{#each error}}
<p>{{msg}}</p>
{{/each}}
