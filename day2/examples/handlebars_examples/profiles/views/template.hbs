<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Template</title>
  </head>
  <body>
    <p>
      {{allstudents}}
      <ul>
        {{allstudents}}
        {{#each students}}
        <li>{{this.first_name}} {{this.last_name}} {{this.email}}</li>
        {{/each}}
      </ul>

      {{malestudents}}
      <ul>
        {{#each males}}
        <li>{{this.first_name}} {{this.last_name}} {{this.email}}</li>
        {{/each}}
      </ul>

      {{femalestudents}}
      <ul>
        {{#each females}}
        <li>{{this.first_name}} {{this.last_name}} {{this.email}}</li>
        {{/each}}
      </ul>


    </p>
  </body>
</html>
