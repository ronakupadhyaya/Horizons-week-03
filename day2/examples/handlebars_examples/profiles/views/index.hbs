<!-- Create a Handlebars template under views that, given an array of students,
displays their first name, last name, and email in a list. -->
<div>
  <span>
    {{#each data}}
    <ul>
      <li>
        {{this.first_name}}+{{this.last_name}}+{{this.email}}
      </li>
    </ul>
    {{/each}}
  </span>
</div>
