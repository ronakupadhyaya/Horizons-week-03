<div>
  <table>
    <tr>
      <th>Student ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Gender</th>
    </tr>
    {{#each students}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.gender}}</td>
    </tr>
    {{/each}}
  </table>
</div>
