<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>WOO HOO</title>
  </head>
  <body>
    <div>
      <h1>WELKOMMEN</h1>

      {{#if maleStudents}}
        <ul>
          {{#each maleStudents}}
            <li> {{this.first_name}} {{this.last_name}} </li>
          {{/each}}
        </ul>
      {{/if}}

      {{#if femaleStudents}}
        <ul>
          {{#each femaleStudents}}
            <li> {{this.first_name}} {{this.last_name}} </li>
          {{/each}}
        </ul>
      {{/if}}

      <ul>
        {{#each students}}
          <li> {{this.first_name}} {{this.last_name}} </li>
        {{/each}}
      </ul>
      
    </div>



  </body>
</html>
